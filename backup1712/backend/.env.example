# PostgreSQL Database Connection URL
# Example: postgresql://user:password@host:port/database
DATABASE_URL=postgresql://postgres:password@localhost:5432/your_database

# Google Maps API Key
# Used for calculating distances and reverse geocoding
GOOGLE_MAPS_KEY=

# GPS Dozor Credentials
# Used for getting truck GPS data.
# You can use either Username/Password OR an API Key.
GPS_DOZOR_URL=https://a1.gpsguard.eu/api/v1
GPS_DOZOR_USERNAME=
GPS_DOZOR_PASSWORD=
GPS_DOZOR_API_KEY=

# Credentials for Trans.eu scraper automation
# These are used by the scraper service to log in.
TRANS_EU_EMAIL=
TRANS_EU_PASSWORD=

# OpenAI API key (if used)
OPENAI_API_KEY=
